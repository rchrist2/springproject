<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="welcomeLoginPane" prefHeight="450.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myproject.controllers.LoginController">
   <children>
      <Label layoutX="128.0" layoutY="99.0" text="Log in" textAlignment="RIGHT">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameText" layoutX="57.0" layoutY="175.0" prefHeight="25.0" prefWidth="211.0" />
      <Label layoutX="57.0" layoutY="149.0" text="Username">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="57.0" layoutY="220.0" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button layoutX="57.0" layoutY="314.0" mnemonicParsing="false" onAction="#signinUser" prefHeight="35.0" prefWidth="211.0" style="-fx-background-color: black;" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Hyperlink layoutX="168.0" layoutY="269.0" onAction="#changeToSignup" text="Forgot Password?" textFill="#110b0b" underline="true">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Hyperlink>
      <ImageView fitHeight="70.0" fitWidth="70.0" layoutX="128.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" />
      <Hyperlink layoutX="253.0" text="Need help?" textFill="#110b0b">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Hyperlink>
      <Button fx:id="closeButton" layoutX="140.0" layoutY="403.0" mnemonicParsing="false" onAction="#closeLogin" style="-fx-background-color: transparent;" text="Close" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="passwordText" layoutX="57.0" layoutY="244.0" prefHeight="25.0" prefWidth="211.0" />
   </children>
</Pane>
